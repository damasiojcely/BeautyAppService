<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="default" extends="struts-default">
        <action name="index">
            <result>index.jsp</result>
        </action>
        <action name="client" class="pe.com.ctaf.beautyapp.actions.ClientAction">
            
        </action>
        <action name="location" class="pe.com.ctaf.beautyapp.actions.LocationAction">

        </action>
        <action name="listowner" class="pe.com.ctaf.beautyapp.actions.OwnerAction">
            <result name="success">listOwner.jsp</result>
        </action>
        <action name="createowner" class="pe.com.ctaf.beautyapp.actions.OwnerAction">
            <result name="input">createOwner.jsp</result>
            <result name="success">listOwner.jsp</result>
        </action>
        <action name="editowner" class="pe.com.ctaf.beautyapp.actions.OwnerAction">
            <result name="input">editOwner.jsp</result>
            <result name="success">listOwner.jsp</result>
        </action>
        <action name="reservation" class="pe.com.ctaf.beautyapp.actions.ReservationAction">

        </action>
        <action name="salon" class="pe.com.ctaf.beautyapp.actions.SalonAction">
            
        </action>
        <action name="schedule" class="pe.com.ctaf.beautyapp.actions.ScheduleAction">
            
        </action>
        <action name="listservice" class="pe.com.ctaf.beautyapp.actions.ServiceAction">
            <result name="success">listService.jsp</result>
        </action>
        <action name="createservices" class="pe.com.ctaf.beautyapp.actions.ServiceAction">
            <result name="input">createService.jsp</result>
            <result name="success">listService.jsp</result>
        </action>
        <action name="editservice" class="pe.com.ctaf.beautyapp.actions.ServiceAction">
            <result name="input">editService.jsp</result>
            <result name="success">listService.jsp</result>
        </action>
        <action name="skill" class="pe.com.ctaf.beautyapp.actions.SkillAction">
            
        </action>
        <action name="stylist" class="pe.com.ctaf.beautyapp.actions.StylistAction">
            
        </action>
        <action name="createuser" class="pe.com.ctaf.beautyapp.actions.UserAction">
            <result name="input">createUser.jsp</result>
            <result name="success">index.jsp</result>
        </action>
        <action name="edituser" class="pe.com.ctaf.beautyapp.actions.UserAction">
            <result name="input">edituser.action</result>
            <result name="success">index.jsp</result>
        </action>
    </package>

</struts>